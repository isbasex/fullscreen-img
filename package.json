  "license": "MIT",
  "author": "baiyun",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://github.com/isbasex/fullscreen-img",
  "repository": {
    "type": "git",
    "url": "https://github.com/isbasex/fullscreen-img.git"
  },
  "module": "dist/fullscreen-img.esm.js",
